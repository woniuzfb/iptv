    #api_key=$($JQ_FILE -r '.epg.modules.common.apiKey' <<< "$bt_prop")

    #bt_prop=$(curl -s -Lm 20 -H "User-Agent: $USER_AGENT_BROWSER" -H "referer: https://www.bt.com/" https://www.player.bt.com/)
    #bt_prop="${bt_prop##*src=\"}"
    #bt_prop="${bt_prop%%\"*}"
